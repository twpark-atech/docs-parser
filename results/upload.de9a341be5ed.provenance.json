    {
      "kind": "text",
      "page_no": null,
      "bbox": null,
      "loc_tokens": null,
      "ref": "text",
      "text": "표 3. 가교형수지 가공제",
      "image_id": null,
      "image_rel_path": null
    },
    {
      "kind": "table",
      "page_no": null,
